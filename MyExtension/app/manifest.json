{
  "name": "My extension",
  "version": "1.0",
  "permissions": [
			"tabs", 
			 "http://*/*",
			  "https://*/*"
],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [ "jquery-1.10.2.js","myscript.js" ],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "img/icon128.png"
  },
 
  "manifest_version": 2
}

